{
  "auth": {
    "docvault": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_NUMBERS",
            "REQUIRES_SYMBOLS",
            "REQUIRES_UPPERCASE"
          ],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL",
          "NAME"
        ],
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "function": {
    "S3Trigger": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId"
          ],
          "category": "auth",
          "resourceName": "docvault"
        },
        {
          "attributes": [
            "BucketName"
          ],
          "category": "storage",
          "resourceName": "s3docvaultfiles"
        },
        {
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ],
          "category": "storage",
          "resourceName": "FileMetadataTable"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "parameters": {
    "AMPLIFY_function_S3Trigger_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "S3Trigger"
        }
      ]
    },
    "AMPLIFY_function_S3Trigger_dynamodbTable": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "S3Trigger"
        }
      ]
    },
    "AMPLIFY_function_S3Trigger_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "S3Trigger"
        }
      ]
    }
  },
  "storage": {
    "FileMetadataTable": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    },
    "s3docvaultfiles": {
      "dependsOn": [],
      "providerPlugin": "awscloudformation",
      "service": "S3"
    }
  }
}